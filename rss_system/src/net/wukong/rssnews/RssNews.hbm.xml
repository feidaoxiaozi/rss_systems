<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="net.wukong.rssnews">

	<class name="net.wukong.rssnews.RssNews" table="rsslist">

		
		<id name="newsId" column="newsId">
			<generator class="identity" />
		</id>
		<property name="title" column="title"/>	
        <property name="url" column="URL"/>			
		<property name="resource" column="resource"/>
		<property name="time" column="time"/>
		<property name="attention" column="attention"/>
		<property name="classes" column="classes"/>
	</class>

</hibernate-mapping>